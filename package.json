{
  "name": "dot.most.box",
  "version": "0.3.0",
  "description": "Pale Blue Dot",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "require": "./dist/server.js",
      "types": "./dist/server.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node server.mjs --port=1976",
    "dev": "npx nodemon server.mjs --port=1976",
    "build-watch": "rollup -c -w",
    "build": "rollup -c",
    "pre-publish": "rm -rf dist && rm -rf node_modules && yarn && npm run build",
    "yarn": "yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/most-people/dot.most.box.git"
  },
  "keywords": [
    "Fully DApp"
  ],
  "author": "most.box",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/most-people/dot.most.box/issues"
  },
  "homepage": "https://github.com/most-people/dot.most.box#readme",
  "dependencies": {
    "ethers": "^6.14.1",
    "ipfs-http-client": "^60.0.1",
    "tweetnacl": "^1.0.3",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@fastify/static": "^8.2.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.15.21",
    "@types/ws": "^8.18.1",
    "fastify": "^5.3.3",
    "rollup": "^4.41.1",
    "rollup-plugin-dts": "^6.2.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  }
}
